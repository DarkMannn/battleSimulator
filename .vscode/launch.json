	"version": "0.2.0",
	"compounds": [
		{
			"name": "Compound",
			"configurations": ["Attach to Node Main", "Attach to Node Child"]
		}
	],
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Node Main",
			"port": 9229,
			"skipFiles": [
				"node_modules/**/*.js"
			],
			"restart": true,
			"timeout": 600000,
			"protocol": "inspector",
			"stopOnEntry": true
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Node Child",
			"port": 9227,
			"skipFiles": [
				"node_modules/**/*.js"
			],
			"restart": true,
			"timeout": 600000,
			"protocol": "inspector",
			"stopOnEntry": true
		},
	]
}
